{{!
    @template local_edwiserreports/breadcrumb

    Display the breadcrumb for reports pages.

    Context variables required for this template:

    Example context (json):
    {
    }
}}
{{#breadcrumb}}
<div class="d-flex flex-wrap" >
    <div id="page-navbar">
        <nav aria-label="Navigation bar">
            <ol class="breadcrumb">
                {{#items}}
                <li class="breadcrumb-item">
                    {{{item}}}
                </li>
                {{/items}}
            </ol>
        </nav>
    </div>
</div>
{{/breadcrumb}}
